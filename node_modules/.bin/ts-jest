#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node_modules/.pnpm/ts-jest@27.1.4_3y7pzupgu3ueocdatbfldfrs3i/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../node_modules/.pnpm/ts-jest@27.1.4_3y7pzupgu3ueocdatbfldfrs3i/node_modules/ts-jest/cli.js" "$@"
fi
