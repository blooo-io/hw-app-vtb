#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node_modules/.pnpm/jest@27.5.1_ts-node@10.7.0/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../../node_modules/.pnpm/jest@27.5.1_ts-node@10.7.0/node_modules/jest/bin/jest.js" "$@"
fi
