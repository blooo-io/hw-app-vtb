#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node_modules/.pnpm/documentation@13.2.4/node_modules/documentation/bin/documentation.js" "$@"
else
  exec node  "$basedir/../../../../../../node_modules/.pnpm/documentation@13.2.4/node_modules/documentation/bin/documentation.js" "$@"
fi
