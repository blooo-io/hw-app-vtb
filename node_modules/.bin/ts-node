#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node_modules/.pnpm/ts-node@10.7.0_33lso24kgdvwdhve7eirdtgycu/node_modules/ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../node_modules/.pnpm/ts-node@10.7.0_33lso24kgdvwdhve7eirdtgycu/node_modules/ts-node/dist/bin.js" "$@"
fi
